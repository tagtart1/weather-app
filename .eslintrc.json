{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "quotes": "off",
    "no-unused-vars": "warn",
    "import/prefer-default-export": "off",
    "comma-dangle": "off",
    "no-return-assign": "off",
    "prefer-destructuring": "off"
  }
}
